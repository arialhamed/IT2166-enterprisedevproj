@page "{id?}"
@model enterprisedevproj.Pages.IS.ViewUserModel
@{
    ViewData["Title"] = "User Details";
}
<h2>User details</h2>
<table class="table table-striped table-borderless table-hover mt-3">
    <tbody>
        <tr>
            <th scope="row">User ID</th>
            <td>@Model.HereUser.Id</td>
        </tr>
        <tr>
            <th scope="row">First Name</th>
            <td>@Model.HereUser.FirstName</td>
        </tr>
        <tr>
            <th scope="row">Last Name</th>
            <td>@Model.HereUser.LastName</td>
        </tr>
        <tr>
            <th scope="row">Account Created IP</th>
            <td>@Model.HereUser.accountCreatedIp</td>
        </tr>
        <tr>
            <th scope="row">Account Created Time</th>
            <td>@Model.HereUser.accountCreatedTime</td>
        </tr>
        <tr>
            <th scope="row">Last Login Time</th>
            <td>@Model.HereUser.lastLoginTime</td>
        </tr>
        <tr>
            <th scope="row">Username</th>
            <td>@Model.HereUser.UserName</td>
        </tr>
        <tr>
            <th scope="row">Normalized Username</th>
            <td>@Model.HereUser.NormalizedUserName</td>
        </tr>
        <tr>
            <th scope="row">Email</th>
            <td>@Model.HereUser.Email</td>
        </tr>
        <tr>
            <th scope="row">Normalized Email</th>
            <td>@Model.HereUser.NormalizedEmail</td>
        </tr>
        <tr>
            <th scope="row">Email Confirmed</th>
            <td>@Model.HereUser.EmailConfirmed</td>
        </tr>
        <tr>
            <th scope="row">Password Hash</th>
            <td>@Model.HereUser.PasswordHash</td>
        </tr>
        <tr>
            <th scope="row">Security Stamp</th>
            <td>@Model.HereUser.SecurityStamp</td>
        </tr>
        <tr>
            <th scope="row">Concurrency Stamp</th>
            <td>@Model.HereUser.ConcurrencyStamp</td>
        </tr>
        <tr>
            <th scope="row">Phone Number</th>
            <td>@Model.HereUser.PhoneNumber</td>
        </tr>
        <tr>
            <th scope="row">Phone Number Confirmed</th>
            <td>@Model.HereUser.PhoneNumberConfirmed</td>
        </tr>
        <tr>
            <th scope="row">Two Factor Authentication Enabled</th>
            <td>@Model.HereUser.TwoFactorEnabled</td>
        </tr>
        <tr>
            <th scope="row">Lockout Date End</th>
            <td>@Model.HereUser.LockoutEnd</td>
        </tr>
        <tr>
            <th scope="row">Lockout Enabled</th>
            <td>@Model.HereUser.LockoutEnabled</td>
        </tr>
        <tr>
            <th scope="row">Access Failed Count</th>
            <td>@Model.HereUser.AccessFailedCount</td>
        </tr>

    </tbody>
</table>
<a class="btn btn-secondary" asp-page="/IS/Main" asp-route-id="users">Cancel</a>
<a class="btn btn-danger text-right" asp-page="/IS/BanUser" asp-route-id="@Model.HereUser.Id">Ban</a>
<a class="btn btn-danger text-right" asp-page="/IS/LockoutUser" asp-route-id="@Model.HereUser.Id">Suspend</a>
<form method="post">
    <button class="btn btn-warning">Send Warning</button>
    <input type="hidden" name="id" value="@Model.HereUser.Id" />
</form>